{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "outils.c",
                "${fileDirname}/commands/builtin.c",
                "${fileDirname}/commands/builtin2.c",
                "${fileDirname}/commands/env.c",
                "${fileDirname}/commands/ext_commands.c",
                "${fileDirname}/redirections/input.c",
                "${fileDirname}/redirections/output.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-L${fileDirname}/../lib/libft",
                "-lft",
                "-lreadline"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}